{
  "manifest_version": 3,
  "name": "SearXNG @ search.daveking.co.nz",
  "version": "1.0",
  "author": "dave@daveking.com",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "48": "searxng48.webp"
  },
  "action": {
    "default_title": "SearXNG"
  },
  "browser_specific_settings": {
    "gecko": {
	  "id": "searchxng@daveking.co.nz",
      "strict_min_version": "64.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "SearXNG",
      "keyword": "@searxng, @searx, @sx",
      "search_url": "https://search.daveking.co.nz/search?q={searchTerms}",
      "favicon_url": "searxng128.webp",
      "suggest_url": "https://search.daveking.co.nz/autocompleter?q={searchTerms}",
      "is_default": true
    }
  }
}
